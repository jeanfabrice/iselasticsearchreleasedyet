{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": [ "error", { "args": "none", "vars": "all" } ],
    "quotes": [ "error", "single", { "avoidEscape": true } ],
    "require-yield": 0,
    "semi": [ 2, "always" ],
    "space-before-function-paren": [ "error", "never" ],
    "strict": [ "error", "global" ]
  }
}
